<%- include('layouts/header.ejs') %>
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header2">
            <small>Apuração da prova de Pesca Variada</small>
        </h1>
    </div>
</div>
<div class="page-data">
    <div class="data-table">
        <table class="table table-striped" style="width: 1500px">
            <thead>
                <tr>
                    <th>Categoria</th>
                    <th>Sorteio</th>
                    <th>Equipe</th>
                    <th>Nome</th>
                </tr>
            </thead>
            <tbody>

                <% if(data){

                    var options = ["Masculino","Feminino","Master","Sênior","Juvenil","Incentivado"];

                for(var i = 0;i < data.length;i++) { %>

                <tr>
                    <td><%=options[data[i].categoria]%></td>
                    <td><%=data[i].sorteio_1a_etapa_variada%></td>
                    <td><%=data[i].nome_equipe%></td>
                    <td><%=(data[i].atleta_id + ' - ' + data[i].nome)%></td>
                </tr>
                <% }

                }else{ %>
                <tr>
                    <td colspan="6">Sem Atletas</td>
                </tr>
                <% } %>
            </tbody>


        </table>
    </div>
</div>

<%- include('layouts/footer.ejs') %>
